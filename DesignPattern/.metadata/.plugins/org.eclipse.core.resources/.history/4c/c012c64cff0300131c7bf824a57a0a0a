public class Meme {
	public static class Lsa {
		int lsa1;
		int lsa2;		
	}
	
	public static class Step {
		Lsa memberLsa;
		
		public Step (int arg1, int arg2) {
			this.memberLsa.lsa1 = arg1;
			this.memberLsa.lsa2 = arg2;
		}
	}
	
	public static void main (String[] args) {
		Step a1 = new Step(1, 3);
		System.out.println(a1.memberLsa.lsa1+" "+a1.memberLsa.lsa2);

	}
	
	
}

